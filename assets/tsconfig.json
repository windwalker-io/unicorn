{
  "extends": [
    //        "@vue/tsconfig/tsconfig.dom.json"
  ],
  "include": [
    "./src/**/*.ts"
  ],
  "exclude": [
    "node_modules/**/*"
  ],
  "types": [
    "reflect-metadata"
  ],
  "compilerOptions": {
    "target": "esnext",
    "moduleResolution": "bundler",
    "esModuleInterop": false,
    "allowJs": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    //        "declaration": true,
    "paths": {
      "@/*": ["./src/*"],
    }
  }
}
